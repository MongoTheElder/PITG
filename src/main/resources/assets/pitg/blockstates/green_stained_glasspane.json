{
  "multipart": [
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "pitg:block/green_stained_glasspane"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "pitg:block/green_stained_glasspane",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "pitg:block/green_stained_glasspane",
        "y": 180
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "pitg:block/green_stained_glasspane",
        "y": 270
      }
    },
    {
      "when": {
        "north": true,
        "west": true
      },
      "apply": {
        "model": "pitg:block/green_stained_glasspane_edge_left"
      }
    },
    {
      "when": {
        "north": true,
        "east": true
      },
      "apply": {
        "model": "pitg:block/green_stained_glasspane_edge_right"
      }
    },
    {
      "when": {
        "west": true,
        "south": true
      },
      "apply": {
        "model": "pitg:block/green_stained_glasspane_edge_right",
        "y": 180
      }
    },
    {
      "when": {
        "east": true,
        "south": true
      },
      "apply": {
        "model": "pitg:block/green_stained_glasspane_edge_left",
        "y": 180
      }
    },
    {
      "when": {
        "east": true,
        "south": true
      },
      "apply": {
        "model": "pitg:block/green_stained_glasspane_edge_right",
        "y": 90
      }
    },
    {
      "when": {
        "north": true,
        "east": true
      },
      "apply": {
        "model": "pitg:block/green_stained_glasspane_edge_left",
        "y": 90
      }
    },
    {
      "when": {
        "west": true,
        "south": true
      },
      "apply": {
        "model": "pitg:block/green_stained_glasspane_edge_left",
        "y": 270
      }
    },
    {
      "when": {
        "west": true,
        "north": true
      },
      "apply": {
        "model": "pitg:block/green_stained_glasspane_edge_right",
        "y": 270
      }
    },
   {
      "when": {
        "OR": 
          [{ "north": true},{
          "west": true}]
        
      },
      "apply": {
        "model": "pitg:block/green_stained_glasspane_corners"
      }
    },
    {
      "when": {
        "OR": [{
          "north": true},{
          "east": true}]
        
      },
      "apply": {
        "model": "pitg:block/green_stained_glasspane_corners",
        "y": 90
      }
    },
    {
      "when": {
        "OR": [{
          "west": true},{
          "south": true}]
        
      },
      "apply": {
        "model": "pitg:block/green_stained_glasspane_corners",
        "y": 270
      }
    },
    {
      "when": {
        "OR": [{
          "east": true},{
          "south": true}]
        
      },
      "apply": {
        "model": "pitg:block/green_stained_glasspane_corners",
        "y": 180
      }
    }
  ]
}